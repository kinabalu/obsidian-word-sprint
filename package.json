{
	"name": "obsidian-word-sprint",
	"version": "0.2.8",
	"description": "Word Sprint for Obsidian plugin for your writing projects like Nanowrimo",
	"main": "main.js",
	"scripts": {
		"dev": "rollup --config rollup.config.js --bundleConfigAsCjs -w",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"build": "rollup --config rollup.config.js --bundleConfigAsCjs --environment BUILD:production"
	},
	"keywords": [],
	"author": "Andrew Lombardi <andrew@mysticcoders.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/kinabalu/obsidian-word-sprint.git"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.7",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/jest": "29.5.12",
		"@types/moment": "^2.13.0",
		"@types/node": "^16.11.1",
		"@types/numeral": "^2.0.5",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/uuid": "^8.3.1",
		"esbuild": "0.21.5",
		"jest": "29.7.0",
		"obsidian": "^1.5.7-1",
		"rollup": "^4.17.2",
		"ts-jest": "29.1.4",
		"tslib": "2.6.3",
		"typescript": "5.4.5"
	},
	"dependencies": {
		"numeral": "^2.0.6",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"uuid": "^10.0.0"
	},
	"jest": {
		"preset": "ts-jest",
		"moduleNameMapper": {
			"src/(.*)": "<rootDir>/src/$1"
		},
		"moduleFileExtensions": [
			"js",
			"ts"
		]
	}
